{
  "expo": {
    "sdkVersion": "52.0.0",
    "name": "match-matters",
    "slug": "match-matters",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
       "expo-image-picker"
    ],
    "assetBundlePatterns": [
      "assets/*"
    ]
  }
}
